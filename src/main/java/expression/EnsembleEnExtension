package expression;
import java.util.ArrayList;

public class EnsembleEnExtension extends NonTerminal {
    ArrayList<Entier> data;
    public void set(Entier expr){
        this.data.add(expr);
    }

    public Entier get(int index){
        if(index<data.size())
            return this.data.get(index);
        else
            return null;
    }

}